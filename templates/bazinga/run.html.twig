{% extends 'base.html.twig' %}

{% block title %}Bazinga{% endblock %}

{% block body %}
    <h1>Getal: {{ number }}</h1>
    {% for i in 1..number %}
        {% if i % 12 == 0 %}
            {{ i }} bazinga!
        {% elseif i % 4 == 0 %}
            {{ i }} bar
        {% elseif i % 3 == 0 %}
            {{ i }} foo
        {% endif %}
    {% endfor %}
{% endblock %}
